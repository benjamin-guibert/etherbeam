{
  "name": "eth-server",
  "author": "Benjamin Guibert <contact@bguibert.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc && node --unhandled-rejections=strict dist/src/index.js | pino-pretty",
    "build": "tsc",
    "start": "yarn run build && node dist/src/index.js",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:watch": "yarn run test --watch --coverage=false",
    "format": "prettier -c '{src,tests}/**/*.{js,ts,md}'",
    "format:w": "prettier --write '{src,tests}/**/*.{js,ts,md}'",
    "lint": "eslint --max-warnings 0 '{src,tests}/**/*.{js,ts,md}'",
    "lint:w": "yarn run lint --fix"
  },
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/jest": "^26.0.14",
    "@types/pino": "^6.3.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "jest": "^26.5.2",
    "pino-pretty": "^4.2.1",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@ethersproject/contracts": "^5.0.5",
    "@ethersproject/providers": "^5.0.14",
    "@ethersproject/solidity": "^5.0.5",
    "@uniswap/sdk": "^3.0.3",
    "async": "^3.2.0",
    "axios": "^0.20.0",
    "pino": "^6.6.1",
    "web3": "^1.3.0"
  }
}
